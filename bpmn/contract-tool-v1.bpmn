      <bpmn:outgoing>Flow_0911t5r</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:endEvent id="Event_0q88ag3" name="End">
      <bpmn:incoming>Flow_0911t5r</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:sequenceFlow id="Flow_0911t5r" sourceRef="Activity_0aoh3am" targetRef="Event_0q88ag3" />
    <bpmn:textAnnotation id="TextAnnotation_0tqoz2q">
      <bpmn:text>1. If Approve, contract will live/active and store to DB.
2. Legal team will send to PM when Decline.</bpmn:text>
    </bpmn:textAnnotation>
