    <bpmn:serviceTask id="Activity_0qizalu" name="Notify Legal" camunda:asyncBefore="true" camunda:type="external" camunda:topic="notify-legal">
      <bpmn:extensionElements>
        <camunda:inputOutput>
          <camunda:inputParameter name="toEmail">test@local.com</camunda:inputParameter>
          <camunda:inputParameter name="subject">Contract  ${contractTitle} Requires Review</camunda:inputParameter>
          <camunda:inputParameter name="body">Hi Legal Team, 
Please review contract for ${contractTitle} created by Procurement Manager.

Contract Type: ${contractType}
Roles: ${roles}
Skills: ${skills}
Offer Price: ${budget}
Contract Start: ${contractStartDate}
Contract End: ${contractEndDate}


Thanks

Best Regards
Procurement Manager</camunda:inputParameter>
        </camunda:inputOutput>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_1id0aht</bpmn:incoming>
      <bpmn:outgoing>Flow_0cws379</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:sequenceFlow id="Flow_0cws379" sourceRef="Activity_0qizalu" targetRef="Activity_11wfe6l" />
    <bpmn:exclusiveGateway id="Gateway_1muxfpz" name="Approved?">
      <bpmn:incoming>Flow_0lv794a</bpmn:incoming>
      <bpmn:outgoing>Flow_0pnc7gd</bpmn:outgoing>
      <bpmn:outgoing>Flow_0kq7lni</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:sequenceFlow id="Flow_0lv794a" sourceRef="Activity_11wfe6l" targetRef="Gateway_1muxfpz" />
    <bpmn:sequenceFlow id="Flow_0pnc7gd" name="Decline" sourceRef="Gateway_1muxfpz" targetRef="Activity_0v97b81">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${approvaldecision == "decline"}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_0kq7lni" name="Approve" sourceRef="Gateway_1muxfpz" targetRef="Activity_0aoh3am">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${approvaldecision == "approve"}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:userTask id="Activity_0aoh3am" name="Store Contract" camunda:formRef="storeContract" camunda:formRefBinding="latest">
